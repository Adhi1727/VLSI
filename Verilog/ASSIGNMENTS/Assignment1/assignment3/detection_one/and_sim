#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f25023ad690 .scope module, "tb_detect" "tb_detect" 2 1;
 .timescale 0 0;
v0x5f25023bea70_0 .net "one", 0 0, L_0x5f25023bef40;  1 drivers
v0x5f25023beb40_0 .var "x", 7 0;
v0x5f25023bec10_0 .net "zero", 0 0, L_0x5f25023bee30;  1 drivers
S_0x5f25023ad820 .scope module, "dut" "detect" 2 5, 3 1 0, S_0x5f25023ad690;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "x";
    .port_info 1 /OUTPUT 1 "zero";
    .port_info 2 /OUTPUT 1 "one";
L_0x5f25023bee30 .functor NOT 1, L_0x5f25023bed10, C4<0>, C4<0>, C4<0>;
v0x5f25023ada50_0 .net *"_ivl_1", 0 0, L_0x5f25023bed10;  1 drivers
v0x5f25023be7b0_0 .net "one", 0 0, L_0x5f25023bef40;  alias, 1 drivers
v0x5f25023be870_0 .net "x", 7 0, v0x5f25023beb40_0;  1 drivers
v0x5f25023be930_0 .net "zero", 0 0, L_0x5f25023bee30;  alias, 1 drivers
L_0x5f25023bed10 .reduce/or v0x5f25023beb40_0;
L_0x5f25023bef40 .reduce/and v0x5f25023beb40_0;
    .scope S_0x5f25023ad690;
T_0 ;
    %vpi_call 2 8 "$monitor", "x=%b zero=%b one=%b", v0x5f25023beb40_0, v0x5f25023bec10_0, v0x5f25023bea70_0 {0 0 0};
    %pushi/vec4 42, 0, 8;
    %store/vec4 v0x5f25023beb40_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5f25023beb40_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x5f25023beb40_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5f25023beb40_0, 0, 8;
    %delay 10, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_detect.v";
    "detect.v";
