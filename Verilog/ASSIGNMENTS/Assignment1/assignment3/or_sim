#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5cc781b4ab70 .scope module, "tb_sum" "tb_sum" 2 1;
 .timescale 0 0;
v0x5cc781b5b4d0_0 .var "a", 0 0;
v0x5cc781b5b5a0_0 .var "b", 0 0;
v0x5cc781b5b670_0 .net "sum", 0 0, L_0x5cc781b5b770;  1 drivers
S_0x5cc781b4ad00 .scope module, "uut" "sum" 2 6, 3 1 0, S_0x5cc781b4ab70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
L_0x5cc781b5b770 .functor OR 1, v0x5cc781b5b4d0_0, v0x5cc781b5b5a0_0, C4<0>, C4<0>;
v0x5cc781b4af30_0 .net "a", 0 0, v0x5cc781b5b4d0_0;  1 drivers
v0x5cc781b5b2f0_0 .net "b", 0 0, v0x5cc781b5b5a0_0;  1 drivers
v0x5cc781b5b3b0_0 .net "sum", 0 0, L_0x5cc781b5b770;  alias, 1 drivers
    .scope S_0x5cc781b4ab70;
T_0 ;
    %vpi_call 2 10 "$display", "time | a b | sum" {0 0 0};
    %vpi_call 2 11 "$monitor", "%4t | %b %b | %b", $time, v0x5cc781b5b4d0_0, v0x5cc781b5b5a0_0, v0x5cc781b5b670_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc781b5b4d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc781b5b5a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc781b5b4d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc781b5b5a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc781b5b4d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc781b5b5a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc781b5b4d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc781b5b5a0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_sum.v";
    "sum.v";
