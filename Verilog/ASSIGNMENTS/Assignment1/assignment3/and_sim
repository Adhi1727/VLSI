    .port_info 5 /OUTPUT 4 "xor_xy";
    .port_info 6 /OUTPUT 4 "xnor_xy";
L_0x5c6cf31ce810 .functor NOT 4, v0x5c6cf31ce4e0_0, C4<0000>, C4<0000>, C4<0000>;
