v0x5c6cf31ce020_0 .net "xor_xy", 3 0, L_0x5c6cf31ceb30;  alias, 1 drivers
v0x5c6cf31ce100_0 .net "y", 3 0, v0x5c6cf31ce740_0;  1 drivers
    .scope S_0x5c6cf317d490;
T_0 ;
