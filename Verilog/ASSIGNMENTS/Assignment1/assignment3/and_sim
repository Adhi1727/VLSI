    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x5c6cf31ce4e0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5c6cf31ce740_0, 0, 4;
