 .timescale 0 0;
v0x5f5166cd6da0_0 .var "i0", 0 0;
v0x5f5166cd6e60_0 .var "i1", 0 0;
v0x5f5166cd6f30_0 .var "sel", 0 0;
v0x5f5166cd7030_0 .net "y", 0 0, L_0x5f5166cd7100;  1 drivers
