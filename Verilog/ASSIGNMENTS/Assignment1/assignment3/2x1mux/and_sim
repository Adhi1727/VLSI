    .port_info 0 /INPUT 1 "sel";
    .port_info 1 /INPUT 1 "i0";
    .port_info 2 /INPUT 1 "i1";
    .port_info 3 /OUTPUT 1 "y";
v0x5f5166cc5910_0 .net "i0", 0 0, v0x5f5166cd6da0_0;  1 drivers
v0x5f5166cd6ad0_0 .net "i1", 0 0, v0x5f5166cd6e60_0;  1 drivers
v0x5f5166cd6b90_0 .net "sel", 0 0, v0x5f5166cd6f30_0;  1 drivers
v0x5f5166cd6c30_0 .net "y", 0 0, L_0x5f5166cd7100;  alias, 1 drivers
L_0x5f5166cd7100 .functor MUXZ 1, v0x5f5166cd6da0_0, v0x5f5166cd6e60_0, v0x5f5166cd6f30_0, C4<>;
    .scope S_0x5f5166cc5530;
T_0 ;
    %vpi_call 2 10 "$monitor", "sel=%b i0=%b i1=%b output=%b ", v0x5f5166cd6f30_0, v0x5f5166cd6da0_0, v0x5f5166cd6e60_0, v0x5f5166cd7030_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f5166cd6f30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f5166cd6da0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f5166cd6e60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f5166cd6f30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f5166cd6da0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f5166cd6e60_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_0;
