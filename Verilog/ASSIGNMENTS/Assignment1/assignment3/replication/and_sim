:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59d2db076e50 .scope module, "tb_replication" "tb_replication" 2 1;
 .timescale 0 0;
v0x59d2db089b20_0 .var "a", 1 0;
v0x59d2db089c00_0 .var "b", 1 0;
v0x59d2db089cd0_0 .var "c", 1 0;
v0x59d2db089dd0_0 .net "y", 6 0, L_0x59d2db08a180;  1 drivers
S_0x59d2db076fe0 .scope module, "dut" "replication" 2 5, 3 1 0, S_0x59d2db076e50;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "a";
    .port_info 1 /INPUT 2 "b";
    .port_info 2 /INPUT 2 "c";
    .port_info 3 /OUTPUT 7 "y";
v0x59d2db077230_0 .net *"_ivl_1", 0 0, L_0x59d2db089ea0;  1 drivers
v0x59d2db089500_0 .net *"_ivl_2", 3 0, L_0x59d2db089fc0;  1 drivers
v0x59d2db0895e0_0 .net *"_ivl_5", 0 0, L_0x59d2db08a0e0;  1 drivers
v0x59d2db0896d0_0 .net "a", 1 0, v0x59d2db089b20_0;  1 drivers
v0x59d2db0897b0_0 .net "b", 1 0, v0x59d2db089c00_0;  1 drivers
v0x59d2db0898e0_0 .net "c", 1 0, v0x59d2db089cd0_0;  1 drivers
v0x59d2db0899c0_0 .net "y", 6 0, L_0x59d2db08a180;  alias, 1 drivers
L_0x59d2db089ea0 .part v0x59d2db089c00_0, 0, 1;
L_0x59d2db089fc0 .concat [ 1 1 1 1], L_0x59d2db089ea0, L_0x59d2db089ea0, L_0x59d2db089ea0, L_0x59d2db089ea0;
L_0x59d2db08a0e0 .part v0x59d2db089cd0_0, 1, 1;
L_0x59d2db08a180 .concat [ 1 4 2 0], L_0x59d2db08a0e0, L_0x59d2db089fc0, v0x59d2db089b20_0;
    .scope S_0x59d2db076e50;
T_0 ;
    %vpi_call 2 8 "$monitor", "a=%b|b=%b|c=%b|y=%b", v0x59d2db089b20_0, v0x59d2db089c00_0, v0x59d2db089cd0_0, v0x59d2db089dd0_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x59d2db089b20_0, 0, 2;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x59d2db089c00_0, 0, 2;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x59d2db089cd0_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x59d2db089b20_0, 0, 2;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x59d2db089c00_0, 0, 2;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x59d2db089cd0_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
